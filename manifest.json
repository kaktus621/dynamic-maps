{
  "name": "__MSG_name__",
  "version": "4",
  "manifest_version": 2,
  "description": "__MSG_description__",
  "default_locale": "en",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://plus.google.com/*"],
      "css": ["clickthrough.css"],
      "js": ["jquery-1.8.1.min.js", "mapsframe.js", "clickthrough.js"],
      "run_at": "document_end"
    }
  ],
  "sandbox": {
    "pages": ["map.html"]
  },
  "web_accessible_resources": ["map.html"]
}
